<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<p id="notice"><%= notice %> </p>
<%= form_tag sessions_path  do %>
	<p>
		<%= label_tag :Username %>
		<%= text_field_tag :username %>
	</p>
	<p>
		<%= label_tag :Password %>
		<%= password_field_tag :password %>
	</p>
	<p class="button">
		<%= submit_tag "Log In" %>
	</p>
<% end %>
